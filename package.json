{
    "name": "@63pokupki/design-system",
    "version": "1.1.157",
    "description": "UI component kit and design-system for 63 Pokupki project",
    "author": {
        "name": "63pokupki"
    },
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
        "dist": "cross-env NODE_ENV=production webpack --config webpack.config.dist.js",
        "libgen": "vue-cli-service build --target lib --name components src/components/index.js --dest libdist",
        "live": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js -w",
        "publish:patch": "npm version patch && git push origin master && npm publish --access public --registry https://registry.npmjs.org",
        "storybook:build": "vue-cli-service storybook:build -c config/storybook",
        "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
        "unit": "vue-cli-service test:unit",
        "unit:watch": "vue-cli-service test:unit --watch",
        "prettier:write": "prettier --write src/**/*.js src/**/*.vue",
        "prettier": "prettier src/**/*.js src/**/*.vue",
        "stylelint": "stylelint 'src/**/*.scss'",
        "stylelint:write": "stylelint 'src/**/*.scss' --fix",
        "eslint": "vue-cli-service lint --no-fix",
        "eslint:write": "vue-cli-service lint",
        "prettify": "npm run prettier:write && npm run eslint:write && npm run stylelint:write"
    },
    "main": "libdist/components.umd.min.js",
    "files": [
        "dist",
        "libdist"
    ],
    "dependencies": {
        "@63pokupki/onboarding-core": "^1.0.6",
        "@babel/runtime": "^7.14.0",
        "assign-deep": "^1.0.1",
        "core-js": "^3.12.1",
        "lodash-es": "^4.17.21",
        "short-uuid": "^3.1.1",
        "vue": "^2.6.14",
        "vue-carousel": "git+https://github.com/nullilac/vue-carousel.git",
        "vue-slider-component": "^3.2.14",
        "vue-the-mask": "^0.11.1"
    },
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@babel/plugin-transform-runtime": "^7.13.10",
        "@babel/preset-env": "^7.13.10",
        "@storybook/addon-a11y": "^6.2.5",
        "@storybook/addon-actions": "6.2.5",
        "@storybook/addon-backgrounds": "6.2.5",
        "@storybook/addon-docs": "6.2.5",
        "@storybook/addon-knobs": "6.2.5",
        "@storybook/addon-links": "6.2.5",
        "@storybook/vue": "6.2.5",
        "@vue/cli-plugin-babel": "~4.5.3",
        "@vue/cli-plugin-eslint": "^4.5.9",
        "@vue/cli-plugin-unit-jest": "^4.5.9",
        "@vue/cli-service": "~4.5.3",
        "@vue/eslint-config-airbnb": "^5.0.2",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/test-utils": "^1.1.1",
        "autoprefixer": "^9.8.6",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "browserslist": "^4.14.0",
        "clipboard": "^2.0.7",
        "copy-webpack-plugin": "^6.0.4",
        "cross-env": "^7.0.2",
        "css-loader": "^4.2.2",
        "css-mqpacker": "^7.0.0",
        "cssnano": "^4.1.10",
        "eslint": "^6.7.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^6.2.2",
        "exports-loader": "^1.1.0",
        "file-loader": "^6.0.0",
        "html-loader": "^1.3.0",
        "html-webpack-plugin": "^4.5.2",
        "husky": "^4.3.0",
        "imports-loader": "^1.1.0",
        "jest": "^26.6.3",
        "jest-serializer-vue": "^2.0.2",
        "lint-staged": "^10.5.2",
        "mini-css-extract-plugin": "^0.11.0",
        "node-sass": "^4.14.1",
        "postcss": "^8.2.10",
        "postcss-cli": "^7.1.1",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.2.0",
        "prismjs": "^1.21.0",
        "sass-loader": "^10.0.1",
        "style-loader": "^1.2.1",
        "stylelint": "^13.12.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-scss": "^3.18.0",
        "svg-sprite-loader": "^5.0.0",
        "svg-url-loader": "^6.0.0",
        "url-loader": "^4.1.0",
        "vue-cli-plugin-storybook": "~2.0.0",
        "vue-jest": "^3.0.7",
        "vue-loader": "^15.9.3",
        "vue-style-loader": "^4.1.3",
        "vue-template-compiler": "^2.6.14",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "jest": {
        "testMatch": [
            "**/*.spec.[jt]s?(x)"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "transform": {
            ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "preset": "@vue/cli-plugin-unit-jest",
        "snapshotSerializers": [
            "jest-serializer-vue"
        ]
    },
    "bugs": {
        "url": "https://github.com/63pokupki/design-system/issues"
    },
    "directories": {
        "doc": "docs"
    },
    "homepage": "https://github.com/63pokupki/design-system",
    "keywords": [
        "ui-kit",
        "design-system",
        "components-kit"
    ],
    "license": "BSD-3-Clause",
    "readme": "ERROR: No README data found!",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/63pokupki/design-system.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && npm run unit"
        }
    },
    "lint-staged": {
        "src/**/*.js": [
            "prettier --write",
            "eslint --fix"
        ],
        "src/**/*.vue": [
            "prettier --write",
            "eslint --fix"
        ],
        "src/**/*.scss": "stylelint --fix"
    }
}
